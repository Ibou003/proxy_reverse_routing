{
  "name": "ros-engine",
  "version": "1.0.0",
  "description": "NodeJs kub Proxy",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "clean": "rm -rf node_modules/",
    "grunt": "grunt",
    "build": "rimraf ./dist && npm run lint && grunt",
    "start": "cross-env NODE_ENV=production PORT=80 node --max-http-header-size=128000 ./dist/index.js",
    "watch": "cross-env NODE_ENV=dev PORT=80 nodemon",
    "lint": "tslint ./src/**/*.ts",
    "mock:api": "json-server --watch ./resources/mock-request-kub.json --port 4000",
    "tslint": "tslint",
    "dev": "concurrently -k -n \"Typescript,NodeJS\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/index.js\""
  },
  "keywords": [
    "ros-engine"
  ],
  "author": "Iloutfi",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^3.4.35",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/errorhandler": "0.0.32",
    "@types/lodash": "^4.14.135",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.11.3",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "grunt": "^1.0.3",
    "grunt-available-tasks": "^0.6.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.19",
    "mocha": "6.0.2",
    "mocha-typescript": "^1.0.23",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.13.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@log4js-node/logstash-http": "^1.0.0",
    "@nguniversal/express-engine": "^8.1.1",
    "@types/express": "^4.16.0",
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "elastic-apm-node": "^2.12.1",
    "express": "^4.17.1",
    "express-generator": "^4.16.1",
    "express-status-monitor": "^1.2.3",
    "handlebars": "^4.1.0",
    "http": "0.0.0",
    "http-proxy": "^1.17.0",
    "http-proxy-middleware": "^0.19.1",
    "install": "^0.12.2",
    "jscoverage": "^0.6.0",
    "json-server": "^0.15.0",
    "lodash": "^4.17.11",
    "log4js": "^4.5.1",
    "log4node": "^0.1.6",
    "logstash-client": "^1.1.1",
    "method-override": "^3.0.0",
    "micromatch": "^3.1.10",
    "morgan": "^1.9.1",
    "node-logstash": "0.0.5",
    "npm": "^6.8.0",
    "request": "^2.88.0",
    "uuid": "^3.3.2",
    "walk": "^2.3.14",
    "winston": "^3.2.1",
